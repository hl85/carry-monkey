  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "antd": "^6.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tslog": "^4.10.2"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.3.0",
    "@eslint/js": "^9.39.1",
    "@types/chrome": "^0.1.32",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "esbuild": "^0.25.12",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
